class Solution {
public:
    int leastInterval(vector<char>& tasks, int n) 
    {
        vector<int>hash(26,0);
        for(int i=0;i<tasks.size();i++)
        {
            hash[tasks[i]-'A']++;
        }
        sort(hash.begin(),hash.end());
        int maxx=hash[25]-1;
        int tot=maxx*n;
        for(int i=24;i>=0;i--)
            tot-=min(hash[i],maxx);
        return tasks.size()+max(0,tot);
    }
};
