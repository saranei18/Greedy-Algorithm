class Solution {
public:
    int findUnsortedSubarray(vector<int>& nums) 
    {
        vector<int>temp=nums;
        sort(nums.begin(),nums.end());
        int start=nums.size();
        int end=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]!=temp[i])
            {
                start=min(start,i);
                end=max(end,i);
            }
        }
        return end-start<=0?0:end-start+1;
    }
};
