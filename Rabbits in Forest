class Solution {
public:
    int numRabbits(vector<int>& answers) 
    {
        unordered_map<int,int>mp;
        int count=0;
        for(int i=0;i<answers.size();i++)
        {
            if(mp[answers[i]])
                mp[answers[i]]--;
            else
            {
                count+=answers[i]+1;
                mp[answers[i]]=answers[i];
            }
        }
        return count;
    }
};
