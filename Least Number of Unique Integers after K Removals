class Solution {
public:
    int findLeastNumOfUniqueInts(vector<int>& nums, int k) 
    {
        unordered_map<int,int>mp;
        priority_queue<int,vector<int>,greater<int>>pq;
        for(int i=0;i<nums.size();i++)
            mp[nums[i]]++;
        for(auto&x:mp)
            pq.push(x.second);
        while(k>=pq.top())
        {
            k-=pq.top();
            pq.pop();
        }
        return pq.size();
    }
};
