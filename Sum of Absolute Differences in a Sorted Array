class Solution {
public:
    vector<int> getSumAbsoluteDifferences(vector<int>& nums) 
    {
        vector<int>ans;
        int rs=0;
        int ls=0;
        int n=nums.size();
        for(int i=0;i<nums.size();i++)rs+=nums[i];
        for(int i=0;i<nums.size();i++)
        {
            rs-=nums[i];
            int temp=abs(i*(nums[i])-ls)+abs(rs-(n-i-1)*nums[i]);
            ls+=nums[i];
            ans.push_back(temp);
        }
        return ans;
    }
};
