class Solution {
public:
    bool makeEqual(vector<string>& words) 
    {
        int size=words.size();
        int hash[26]={0};
        for(int i=0;i<words.size();i++)
        {
            for(int j=0;j<words[i].length();j++)
            {
                hash[words[i][j]-'a']++;
            }
        }
        for(int i=0;i<26;i++)
        {
            if(hash[i]%size!=0)return false;
        }
        return true;
            
    }
};
