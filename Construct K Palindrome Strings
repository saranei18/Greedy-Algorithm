class Solution {
public:
    bool canConstruct(string s, int k) 
    {
        if(s.length()<k)return false;
        int hash[26]={0};
        for(int i=0;i<s.length();i++)
            hash[s[i]-'a']++;
        int count=0;
        for(int i=0;i<26;i++)
        {
            if(hash[i]%2!=0)count++;
        }
        return count<=k;
    }
};
