class Solution {
public:
    bool mergeTriplets(vector<vector<int>>& trip, vector<int>& t) 
    {
        int a=0;
        int b=0;
        int c=0;
        
        for(int i=0;i<trip.size();i++)
        {
            if(trip[i][0]==t[0] && trip[i][1]<=t[1] && trip[i][2]<=t[2])a=1;
            if(trip[i][1]==t[1] && trip[i][0]<=t[0] && trip[i][2]<=t[2])b=1;
            if(trip[i][2]==t[2] && trip[i][0]<=t[0] && trip[i][1]<=t[1])c=1;
        }
        return (a&&b&&c);
    }
};
