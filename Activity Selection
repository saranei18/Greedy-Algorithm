#include<iostream>
#include<algorithm>
#include<vector>
#include<string>
using namespace std;
static bool comp(vector<int>& a,vector<int>& b)
{
    return a[1]<b[1];
}
int main()
{
    vector<vector<int>>nums{{1,2},{3,4},{0,6},{5,7},{8,9},{5,9}};

    sort(nums.begin(),nums.end(),comp);


    for(int i=0;i<nums.size();i++)
    {
        for(int j=0;j<nums[0].size();j++)cout<<nums[i][j]<<"  ";
        cout<<endl;
    }
    vector<int>res;
    res.push_back(nums[0][0]);
    int ends=nums[0][1];
    for(int i=1;i<nums.size();i++)
    {
        if(nums[i][0]>=ends)
        {
            res.push_back(nums[i][0]);
            ends=nums[i][1];
        }
    }

    for(int i=0;i<res.size();i++)cout<<res[i]<<"  ";
}
