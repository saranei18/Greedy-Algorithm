class Solution {
public:
    vector<int> prevPermOpt1(vector<int>& arr) 
    {
        int n=arr.size();
        int i;
        for(i=n-1;i>0;i--){
            if(arr[i]<arr[i-1])
                break;
        }
        if(i==0)return arr;
        
        for(int j=n-1;j>=i;j--)
        {
            if(arr[i-1]>arr[j] and arr[j]!=arr[j-1])
            {
                swap(arr[i-1],arr[j]);
                break;
            }
        }
        return arr;
    }
};
