#include<iostream>
#include<vector>
#include<algorithm>
#include<stdio.h>
#include<string>
using namespace std;

typedef struct Knapsack
{
    int profit;
    int weight;
    double pwratio;
}ks;
ks k[] ={ {10, 2},{5, 3},{15, 5},{7, 7},{6, 1},{18,4},{3,1} };

void pwr()
{
    for(int i=0;i<7;i++)
        k[i].pwratio = 1.0 * k[i].profit/k[i].weight;
}
void sorting()
{
    for(int i=0;i<7;i++)
    {
        for(int j=0;j<7-i-1;j++)
        {
            if(k[j].pwratio <= k[j+1].pwratio)
            {
                swap(k[j],k[j+1]);
            }
        }
    }
}
double greedy(int capacity)
{
    double profits=0;

    int i = 0;
    for(i=0;i<7;i++)
    {
        if(capacity > 0 && k[i].weight <= capacity)
        {
            profits+=k[i].profit;
            capacity -=k[i].weight;
        }
        else
            break;
    }
    if(capacity > 0)
    {
        profits += k[i].profit * (1.0 * capacity/k[i].weight);
    }
    return profits;
}
int main()
{
    pwr();
    sorting();
    int capacity = 15;
    printf("%lf",greedy(capacity));
    return 0;
}

