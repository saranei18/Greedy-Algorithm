class Solution {
public:
    vector<int> minSubsequence(vector<int>& nums) 
    {
        sort(nums.begin(),nums.end(),greater<int>());
        vector<int>res;
        int sum=0;
        for(int i=0;i<nums.size();i++)sum+=nums[i];
        int nsum=0;
        for(int i=0;i<nums.size();i++)
        {
            res.push_back(nums[i]);
            nsum+=nums[i];
            sum-=nums[i];
            if(nsum>sum)break;
        }
        return res;
    }
};
