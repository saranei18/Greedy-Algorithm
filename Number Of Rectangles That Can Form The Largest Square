class Solution {
public:
    int countGoodRectangles(vector<vector<int>>& rectangles) 
    {
        int count=0;
        vector<int>temp;
         for(int i=0;i<rectangles.size();i++)
         {
             temp.push_back(min(rectangles[i][0],rectangles[i][1]));
         }
        int num=*max_element(temp.begin(),temp.end());
        for(int i=0;i<temp.size();i++)
        {
            if(num==temp[i])count++;
        }
        return count;
    }
};
