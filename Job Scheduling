#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;

typedef struct JoBSch
{
    int deadline;
    int profit;
}JS;
JS j[]={{7,15},{2,20},{5,30},{3,18},{4,18},{5,10},{2,23},{7,16}};

void sorts(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int k=0;k<n-i-1;k++)
        {
            if(j[k].profit <=  j[k+1].profit)
                swap(j[k],j[k+1]);
        }
    }
}
int greedy(int max_dl,int n)
{
    int profits=0;
    int gant_chart[max_dl];
    for(int i=0;i<max_dl;i++)
        gant_chart[i]=0;
    sorts(n);

    for(int i=0;i<n;i++)
    {
        int k=j[i].deadline-1;
        while(k>=0)
        {
            if(gant_chart[k]==0)
            {
                gant_chart[k]=j[k].profit;
                profits+=j[k].profit;
                break;
            }
        }
    }
    return profits;
}
int main()
{
    int n=sizeof(j)/sizeof(j[0]);
    int max_dl=0;
    for(int i=0;i<n;i++)
        max_dl = max(max_dl,j[i].deadline);

    cout<<greedy(max_dl,n);
}
